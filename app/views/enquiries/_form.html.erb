<!-- START CONTAINER -->
<div class="page-container row-fluid container-fluid">
  <!-- SIDEBAR - START -->
    <%= render 'layouts/sidebar' %>
  <!--  SIDEBAR - END -->
  <!-- START CONTENT -->
  <section id="main-content" class=" ">
    <section class="wrapper main-wrapper row" style=''>
      <div class='col-xs-12'>
        <div class="page-title">
          <div class="pull-left">
            <h1 class="title">Add an Enquiry</h1> </div>
          <div class="pull-right hidden-xs">
            <ol class="breadcrumb">
              <li>
                <a href="index.html"><i class="fa fa-home"></i>Home</a>
              </li>
              <li>
                <a href="crm-tickets.html">Enquaries</a>
              </li>
              <li class="active">
                <strong>Add Enquiry</strong>
              </li>
            </ol>
          </div>
        </div>
      </div>
      <div class="clearfix"></div>
      <div class="col-xs-12">
        <section class="box ">
          <header class="panel_header">
            <h2 class="title pull-left">Basic Info</h2>
   <!--          <div class="actions panel_actions pull-right">
              <a class="box_toggle fa fa-chevron-down"></a>
              <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
              <a class="box_close fa fa-times"></a>
            </div> -->
          </header>
          <div class="content-body">
            <div class="row">
              <%= simple_form_for(enquiry) do |f| %>
                  <% if enquiry.errors.any? %>
                    <div id="error_explanation">
                      <h2>
                        <%= pluralize(enquiry.errors.count, "error") %> prohibited this enquiry from being saved:
                      </h2>
                      <ul>
                        <% enquiry.errors.full_messages.each do |message| %>
                          <li><%= message %></li>
                        <% end %>
                      </ul>
                    </div>
                  <% end %>
                <div class="col-xs-12 col-sm-9 col-md-8">
                  <div class="form-group">
                    <label class="form-label" for="field-11128">Category</label>
                    <span class="desc"></span>
                    <div class="controls">
                      <%= f.association :category, :collection => @categories, label: false, wrapper: false, :include_blank => false, class: 'form-control' %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label">Deadline</label>
                    <span class="desc">e.g. "04/03/2015"</span>
                    <div class="controls">
                    <%= f.text_field :deadline, class: "form-control datepicker", 'data-format' => "mm/dd/yyyy" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label" for="field-129990">Title</label>
                    <span class="desc"></span>
                    <div class="controls">
                      <%= f.input_field :title, class: "form-control" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label" for="field-19575">Budget</label>
                    <span class="desc"></span>
                    <div class="controls">
                      <%= f.input_field :budget, class: "form-control" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label" for="field-19575">Max Applications</label>
                    <span class="desc"></span>
                    <div class="controls">
                      <%= f.input_field :max_applications, class: "form-control" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label">Email</label>
                    <span class="desc"></span>
                    <div class="controls">
                      <%= f.input_field :email, class: "form-control" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label">Mobile</label>
                    <span class="desc">e.g. "(534) 253-5353"</span>
                    <div class="controls">
                      <%= f.input_field :mobile, class: "form-control",'data-mask' => "phone",placeholder: "(999) 999-9999" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label">Contacts</label>
                    <span class="desc"></span>
                    <div class="controls">
                      <%= f.text_area :contact_details, class: "form-control autogrow" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label class="form-label">Description</label>
                    <span class="desc"></span>
                    <div class="controls">
                      <%= f.text_area :job_description, class: "form-control autogrow" %>
                    </div>
                  </div>
                </div>
                <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                  <div class="text-left">
                    <%= f.button :submit, 'Post', class: "btn btn-primary" %>
                    <button type="button" class="btn">Cancel</button>
                  </div>
                </div>
              <% end %>
            </div>
          </div>
        </section>
      </div>
    </section>
  </section>
